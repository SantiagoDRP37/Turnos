@model Turnos.Models.Especialidad

<h3>Esta seguro de Eliminar el siguiente registro?</h3>
<div class="row">
    <div class="col s12 m6">
        <div class="card hoverable blue darken-1">
            <div class="card-content white-text">
                <dl>
                    <dt class="card-title">
                        @Html.DisplayNameFor(model => model.Descripcion)    
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.Descripcion)
                    </dd>
                </dl>
            </div>
            <div class="card-action blue-grey darken-2">
            <form asp-action="Delete">
                <input type="hidden" asp-for="IdEspecialidad"/>
                <input type="submit" class="waves-effect waves-light btn-small orange lighten-2" value="Eliminar">
                <a asp-action="Index" class="right">Volver</a>
            </form>
            </div>
        </div>
    </div>
    
</div> 