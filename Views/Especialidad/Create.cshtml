@model Turnos.Models.Especialidad

<h4>Especialidad</h4>

<div class="row">
    <form asp-action="Create" class="col s12">
        <div asp-validation-summary="ModelOnly"></div>
        <div class="input-field col s12">
            <label asp-for="Descripcion"></label>
            <input asp-for="Descripcion">
            <span asp-validation-for="Descripcion"></span>
        </div>
        
        <div class="row">
            <div class="col s6 m6">
                    
                <button class="btn waves-effect waves-light cyan accent-4" type="submit" >Grabar
                    <i class="material-icons right">send</i>
                 </button>
            </div>
            <div class="col s6 m6">
                <a class="waves-effect waves-light btn grey darken-4" asp-action="Index"><i class="material-icons left">undo</i>Volver</a>
            </div>

        </div>
    </form>
</div>
